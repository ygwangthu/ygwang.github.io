net: "train_val_total.prototxt"
base_lr: 5e-5
momentum: 0.9
weight_decay: 0.0005
# The learning rate policy
lr_policy: "fixed"
gamma: 0.3
# stepsize to decrease learning rate. This should depend on your dataset size
stepsize: 50000
# Display every 100 iterations
display: 5
# The maximum number of iterations
max_iter: 300000
# snapshot intermediate results
snapshot: 1000
snapshot_prefix: "D:/handtrain_total/hand_train"
# solver mode: CPU or GPU
solver_mode: GPU